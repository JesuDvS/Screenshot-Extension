{
  "name": "Screenshot-Extension",
  "description": "A extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "sidePanel",
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_icon": "Screenshot-Extension2.png"
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}